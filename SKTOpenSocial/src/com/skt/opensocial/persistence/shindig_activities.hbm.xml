<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.skt.opensocial.persistence.Activity" table="SHINDIG_ACTIVITIES">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="appId" column="APPID" />
		<property name="body" column="BODY" />
		<property name="bodyId" column="BODYID" />
		<property name="externalId" column="EXTERNALID" />
		<property name="activityId" column="ACTIVITY_ID" />
		<property name="updated" column="UPDATED" />
		<property name="postedTime" column="POSTEDTIME" />
		<property name="priority" column="PRIORITY" />
		<property name="streamFaviconUrl" column="STREAMFAVICONURL" />
		<property name="streamSourceUrl" column="STREAMSOURCEURL" />
		<property name="streamTitle" column="STREAMTITLE" />
		<property name="streamUrl" column="STREAMURL" />
		<property name="title" column="TITLE" />
		<property name="titleId" column="TITLEID" />
		<property name="url" column="URL" />
		<property name="userId" column="USERID" />
	</class>
</hibernate-mapping>
