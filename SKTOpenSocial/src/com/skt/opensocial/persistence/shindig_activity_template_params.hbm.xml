<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.skt.opensocial.persistence.TemplateParam" table="SHINDIG_ACTIVITIES">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="paramKey" column="MIMETYPE" />
		<property name="paramValue" column="TYPE" />
		<property name="activityId" column="ACTIVITY_ID" />
	</class>
</hibernate-mapping>
