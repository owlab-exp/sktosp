<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="developer" namespace="/developer" extends="default">
		<action name="ListGadgets" class="com.skt.opensocial.developer.ListGadgetsAction">
			<result name="success">/developer/DeveloperListGadgets.jsp</result>
		</action>
		<action name="RegisterGadget">
			<result>/developer/DeveloperRegisterGadget_Source.jsp</result>
		</action>
		<action name="PreviewGadget">
			<result>/developer/DeveloperPreviewGadget.jsp</result>
		</action>
		<action name="ModifyGadget">
			<result>/developer/DeveloperModifyGadget_Source.jsp</result>
		</action>
		<action name="ViewGadgetReview">
			<result>/developer/DeveloperViewGadgetReview.jsp</result>
		</action>
	</package>
</struts>